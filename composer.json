{
    "name": "drewlabs/storage-core",
    "description": "Core classes for storage HTTP API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\Storage\\": "src/"
        },
        "files": [
            "./src/proxies.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Storage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "drewlabs/filesystem": "^0.2.0",
        "drewlabs/psr7-stream": "^1.0|^2.0",
        "drewlabs/core-helpers": "^0.2.0",
        "drewlabs/datauri": "^0.2.0",
        "drewlabs/php-value": "^0.2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.5"
    }
}